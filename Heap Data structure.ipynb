    "import heapq\n",
    "arr = [22,4,5,6,88,9]\n",
    "heapq.heapify(arr)\n",
    "print(arr)\n",
    "heapq.heappush(arr,3)\n",
    "print(arr)\n",
    "heapq.heappop(arr)\n",
    "print(arr)\n",
    "print(heapq.nsmallest(3,arr))\n",
    "print(heapq.nlargest(10,arr))"
   ]
